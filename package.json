{
  "name": "kind-ui",
  "version": "0.2.0",
  "description": "kind ui",
  "main": "dist/kind.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config build/webpack.prod.conf.js",
    "minifyjs": "cross-env NODE_ENV=production webpack --progress --config build/webpack.minify.conf.js",
    "postbuild": "npm run minifyjs && gulp buildscss",
    "build:demo": "cross-env NODE_ENV=production IS_DEMO=true webpack --progress --config build/webpack.demo.conf.js",
    "analyze:build": "cross-env report=true npm run build",
    "open:umd": "http-server -o test-umd.html"
  },
  "keywords": [
    "kind-ui",
    "vue ui library",
    "ui",
    "vue"
  ],
  "author": "tusi666",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cumt-robin/kind-ui.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "bezier-easing": "^2.1.0",
    "core-js": "^3.5.0",
    "popper.js": "^1.16.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "eslint": "^6.7.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "portfinder": "^1.0.25",
    "sass-loader": "^8.0.0",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "sideEffects": false
}
